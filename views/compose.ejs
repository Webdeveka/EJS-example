<%- include('header') -%>



<form action="/compose" class="box" method="post">
    <h1>Compose</h1>
    <div class="title">
        <label for="#text">Title</label>
    </div>
    <div>
        <input id="text" type="text" name="newTitle" placeholder="New note">
    </div>
    <div class="title">
        <label for="#text">Post</label>
    </div>
    <div>
        <textarea name="newPost" id="textarea" cols="100%" rows="5"></textarea>
    </div>
    <div>
    <button type="submit" name="note">Publish</button>
    </div>
</form>

